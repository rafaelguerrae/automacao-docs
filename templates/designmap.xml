    <idPkg:Tags src="XML/Tags.xml" />
    
    <!-- Camada -->
    <Layer Self="ucc" Name="Camada 1" Visible="true" Locked="false" IgnoreWrap="false" ShowGuides="true" LockGuides="false" UI="true" Expendable="true" Printable="true">
